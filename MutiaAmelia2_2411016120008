package tipedata;


public class UcapanVar {
    public static void main(String[] args) {
        String salam = "Halo ULM";
        String visiUlm = "ULM Berdaya Saing dan Unggul di Bidang Lahan Basah";
        String visiIlkom = "Prodi Ilkom ULM UNggul dengan Teknologi Kekinian";
               
        System.out.println(salam);
        System.out.println(visiUlm);
        System.out.println(visiIlkom);
        System.out.println("------");
    }
}
